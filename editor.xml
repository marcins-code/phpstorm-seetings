<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Droid Sans Mono for Powerline" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Regular" />
    <option name="LINE_SPACING" value="1.5" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="JetBrains Mono" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="docker.inlay.hints" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>